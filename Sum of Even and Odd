#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n;
    
    int sumodd=0, sumeven=0;
    
    while ( n > 0){
        int l = n % 10;
        if(l % 2 == 0){
            sumeven += l;
        }
        else{
            sumodd +=l;
        }
        n = n/10;
    }
    cout<<sumeven<<" "<<sumodd;
    
    
    
    return 0;
}
